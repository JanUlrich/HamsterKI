/*class*/public class Feld {
    private boolean mauer;
    private int koerner;//Prozentchance auf KÃ¶rner
	
	public Feld(){
		this.mauer = false;
		this.koerner = 25;
	}
	
	public Feld(boolean mauer, int koerner){
		this.mauer = mauer;
		this.koerner = koerner;
	}

	public void setMauer(boolean mauer){
		this.mauer = mauer;
	}
	
	public void setKoerner(int koerner) {
		this.koerner = koerner;
	}
	
	public int getKornChance(){
		if(mauer)return 0;
		return koerner;
	}
}
