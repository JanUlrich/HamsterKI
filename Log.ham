class Log {
	public static boolean DEBUG = false;

	/**
	 * Gibt eine Nachricht aus, wenn das Logging aktiviert ist
	 */
	public static void write(String s) {
		if(!DEBUG)
			return;
		Hamster.getStandardHamster().schreib(s);
	}
}